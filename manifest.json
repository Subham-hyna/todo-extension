{
    "name" : "TO-DO",
    "description" : "Best TO-DO Extensions having the capability of reminding you of the left over tasks",
    "version" : "1.0",
    "manifest_version" : 3,
    "action" : {
        "default_popup" : "popup.html"
    },
    "background" : {
        "service_worker" : "background.js"
    },
    "permissions" : ["tabs" , "activeTab", "scripting" , "storage"],
    "host_permissions" : ["<all_urls>"],
    "icons":{
        "16" : "/images/todo_16.png",
        "32" : "/images/todo_32.png",
        "48" : "/images/todo_48.png"
    }
}